    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <div class="logo">
        <div align="right">By Alexander Bjeldanes</div>
        <div><h1>CSC317 Photos</h1></div>
    </div>
    <div class="topnav">
        
        {{> nav}}

        
    </div>
    

    {{#if logged}}
        <script>
            let ele = document.getElementById('logout-button');
            if (ele) {
                ele.onclick = (even) => {
                    fetch('/users/logout', {
                        method: "POST"
                    })
                    .then((data) => {
                        location.replace('/'); 
                    })
                }
            }
        </script>
    {{/if}}

</body>
</html>
